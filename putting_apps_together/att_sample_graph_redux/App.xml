<?xml version="1.0" encoding="UTF-8"?>
<app height="auto" id="att_sample_graph_redux" jsclass="Presto.BasicApp"
     name="att_sample_graph_redux"
     refreshinterval="0"
     sandbox="false"
     width="auto">
	  <title>att_sample_graph_redux</title>
	  <author>JackBe Presto</author>
	  <description/>
	  <properties>
		    <property datatype="boolean" defaultvalue="true" label="Enable Pagination"
                name="presto.showPaginationControls"
                required="true"/>
		    <property datatype="enum" defaultvalue="20" label="Rows per Page"
                name="presto.rowsPerPage"
                possiblevalues="5,10,20,50,75,100,200,All"/>
		    <property datatype="enum" defaultvalue="gray" label="Theme" name="presto.appTheme"
                possiblevalues="gray,apple,green,black,blue,yellow,mint,gblue,brown,red"
                required="true"/>
	  </properties>
	  <views>
		    <view elemId="6" enable="true" isdefault="false" name="bar chart"/>
	  </views>
	  <presto-meta name="service" type="text">att_sample_mashup_nohash</presto-meta>
	  <presto-meta name="operation" type="text">Invoke</presto-meta>
	  <presto-meta name="serviceuri" type="text">/presto/edge/api/rest/att_sample_mashup_nohash/Invoke?x-presto-resultFormat=json</presto-meta>
	  <presto-meta name="contenttype" type="text">application/x-www-form-urlencoded</presto-meta>
	  <presto-meta name="showsort" type="text">false</presto-meta>
	  <presto-meta name="showfilter" type="text">false</presto-meta>
	  <presto-meta name="postbody"><![CDATA[[object Object]]]></presto-meta>
	  <presto-meta name="viewconfig"><![CDATA[[{"viewType": "barChart", "title": "Bar Chart", "description": "", "events": ["element-click"], "minHeight": 220, "categories": {"label": "name", "value": "name"}, "series": [{"label": "sales", "value": "sales"}], "visualization": "2d", "stack": "no", "scroll": "no", "columns": [{"name": {"label": "name", "value": "name"}}, {"name": [{"label": "sales", "value": "sales"}]}], "name": "bar chart", "chart-type": "bar", "attributes": {"caption": "City Sales", "numberSpinner": "0", "baseFontSize": "10", "outCnvBaseFontSize": "10", "showBorder": "0", "baseFont": "Verdana", "outCnvBaseFont": "Verdana", "formatNumberScale": "1", "formatNumber": "1", "forceDecimals": "0", "decimalSeparator": ".", "thousandSeparator": ",", "logoPosition": "TL", "showValues": "1", "showLabels": "1", "showToolTip": "1", "seriesNameInToolTip": "0", "showToolTipShadow": "0", "showLegend": "1", "legendPosition": "BOTTOM", "legendAllowDrag": "0", "legendMarkerCircle": "0", "reverseLegend": "0", "legendShadow": "1", "palette": "3", "animation": "1", "bgColor": "FF0000", "bgAlpha": "0", "borderAlpha": "", "borderThickness": "", "bgRatio": "", "bgAngle": "180", "logoAlpha": "", "logoScale": "", "legendBgAlpha": "", "legendBorderAlpha": ""}, "repeating-element": "/branch/city"}]]]></presto-meta>
	  <presto-meta name="showviewtitle" type="text">true</presto-meta>
	  <presto-meta name="layout" type="text">tab</presto-meta>
	  <presto-meta name="presto.desktopCompatible" type="text">true</presto-meta>
	  <presto-meta name="presto.phoneCompatible" type="text">false</presto-meta>
	  <presto-meta name="presto.tabletCompatible" type="text">false</presto-meta>
	  <presto-meta name="presto.shareable" type="text">true</presto-meta>
	  <presto-meta name="presto.allowOpenNewWindow" type="text">true</presto-meta>
	  <topics>
		    <topic datatype="object" name="rowselect" subscribe="true">
			      <properties>
				        <property datatype="string" name="sales" required="false"/>
				        <property datatype="string" name="name" required="false"/>
				        <property datatype="string" name="[object Object]" required="false"/>
			      </properties>
		    </topic>
		    <topic datatype="object" name="dataTable.rowselect" publish="true">
			      <properties>
				        <property datatype="string" name="sales" required="false"/>
				        <property datatype="string" name="name" required="false"/>
			      </properties>
		    </topic>
	  </topics>
	  <requires>
		    <require name="basic-app" type="library"/>
		    <require src="js/app_extension.js" type="script"/>
		    <require src="js/extension_config.js" type="script"/>
	  </requires>
	  <dependson>
		    <resource name="att_sample_mashup_nohash" operation="Invoke" type="service"/>
	  </dependson>
</app>